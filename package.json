{
  "version": "3.0.0",
  "name": "gatsby-v2-starter-lumen",
  "description": "Simple starter for Gatsby",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "rm -rf public",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy": "rm -rf public && gatsby clean && gatsby build --prefix-paths",
    "new": "./scripts/article.rb",
    "draft": "./scripts/draft_list.rb print",
    "clearDraft": "./scripts/clear_draft.rb",
    "publish": "./scripts/publish.rb"
  },
  "repository": "git+https://github.com/alxshelepenok/gatsby-starter-lumen.git",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "theme",
    "starter"
  ],
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.5",
    "gatsby": "^2.18.17",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-feed": "^2.3.6",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-google-fonts": "latest",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sass": "^5.1.0",
    "gatsby-plugin-sharp": "^2.2.10",
    "gatsby-plugin-sitemap": "^2.2.5",
    "gatsby-remark-copy-linked-files": "^2.1.4",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.3.4",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.11",
    "gatsby-transformer-sharp": "^2.1.3",
    "lodash": "^4.17.15",
    "node-sass": "6.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^1.1.0",
    "lost": "^8.3.1",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.18.2"
  }
}
